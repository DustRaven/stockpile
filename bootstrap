files = {
  "startup",
  "tablePersistence",
  "userIo",
  "stockpile/craftingRequester",
  "stockpile/itemSearcher",
  "stockpile/settingsPersistence"
}

fs.mkdir("/stockpile")

for _, file in ipairs(files) do
  fs.move("/downloads/stockpile/" .. file, "/" .. file)
end

fs.delete("/downloads")
shell.run("startup")