files = {
  "startup",
  "tablePersistence",
  "userIo",
  "stockpile"
}

for _, file in ipairs(files) do
  local from = "/downloads/stockpile/" .. file
  local to = "/" .. file
  print(from .. "-->" .. to)

  if fs.exists(to) then
    fs.delete(to)
  end
  fs.move(from, to)
end

fs.delete("/downloads")
shell.run("startup")